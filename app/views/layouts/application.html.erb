      <%= javascript_include_tag "flot/excanvas.min.js", 'data-turbolinks-track' => true   %>
      <%= stylesheet_link_tag    'public/ie8', media: 'all', 'data-turbolinks-track' => true   %>
    <![endif]-->
</head>
<body class="<%=controller_name%> <%=action_name%>">
  <header class="navbar navbar-static-top navbar-inverse">
    <a href="https://github.com/inocode/dia-de-treinamento"><img style="position: absolute; top: 0; right: 0; border: 0;" src="https://camo.githubusercontent.com/38ef81f8aca64bb9a64448d0d70f1308ef5341ab/68747470733a2f2f73332e616d617a6f6e6177732e636f6d2f6769746875622f726962626f6e732f666f726b6d655f72696768745f6461726b626c75655f3132313632312e706e67" alt="Fork me on GitHub" data-canonical-src="https://s3.amazonaws.com/github/ribbons/forkme_right_darkblue_121621.png"></a>
    <div class="navbar-header">
      <button class="navbar-toggle collapsed" type="button" data-toggle="collapse" data-target=".bs-navbar-collapse">
        <span class="sr-only">Toggle navigation</span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </button>
      <%= link_to "DIA DE TREINAMENTO", root_path, class: "navbar-brand" %>
    </div>
    <nav class="collapse navbar-collapse bs-navbar-collapse">
     <ul class="nav navbar-nav navbar-right menu-events">
        <li class="dropdown">
          <a href="#" class="dropdown-toggle" data-toggle="dropdown">Eventos <span class="caret"></span></a>
          <ul class="dropdown-menu" role="menu">
            <li><%= link_to "13.06.2015 - HTTP & UX", http_ux_eventos_path %></li>
            <li class="divider"></li>
            <li><%= link_to "09.04.2015 - Desenvolvimento Mobile", desenvolvimento_mobile_eventos_path %></li>
            <li><%= link_to "11.03.2015 - Clean Code", cleancode_eventos_path %></li>
            <li role="presentation" class="divider"></li>
          </ul>
        </li>
      </ul>
    </nav>
  </header>
  <%= yield %>
  <footer>
    <div class="row">
      <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 text-center">
        Quer nos ajudar nos próximos eventos ou submeter uma palestra? Entre em contato <a href="mailto:contato@inocode.com.br">contato@inocode.com.br</a>
      </div>
    </div>
  </footer>

